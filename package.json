{
  "name": "koa-sav",
  "version": "1.0.0",
  "description": "sav middleware for koa",
  "main": "src/index.js",
  "scripts": {
    "bss": "browser-sync start",
    "bsr": "browser-sync reload",
    "svr": "node .",
    "mon": "set PORT=8000 && nodemon --ext \"js json html\" --exec \"npm run bsr && npm run svr\"",
    "start": "npm-run-all -p mon bss",
